{
  "manifest_version": 2,
  "name": "Autoconsent",
  "version": "2021.10.12",
  "background": {
    "scripts": [
      "background.bundle.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.bundle.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "<all_urls>",
    "cookies",
    "tabs",
    "storage",
    "webNavigation"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "autoconsent@sammacbeth.eu"
    }
  },
  "page_action": {
    "browser_style": true,
    "default_title": "Autoconsent"
  }
}