{
  "name": "activobank.pt",
  "prehideSelectors": ["aside#cookies,.overlay-cookies"],
  "detectCmp": [
    { "exists": "aside#cookies .cookies-btn" }
  ],
  "detectPopup": [
    { "visible": "aside#cookies #submitCookies" }
  ],
  "optIn": [
    { "waitForThenClick": "#cookies #submitCookies" }
  ],
  "optOut": [
    { "waitForThenClick": "#cookies #rejectCookies" }
  ]
}
