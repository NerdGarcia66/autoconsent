{
    "name": "microsoft",
    "prehideSelectors": ["#wcpConsentBannerCtrl"],
    "detectCmp": [{ "exists": "#wcpConsentBannerCtrl" }],
    "detectPopup": [{ "exists": "#wcpConsentBannerCtrl" }],
    "optOut": [{ "eval": "Array.from(document.querySelectorAll('div > button')).filter(el => el.innerText === 'Reject')[0].click()" }],
    "optIn": [{ "eval": "Array.from(document.querySelectorAll('div > button')).filter(el => el.innerText === 'Accept')[0].click()" }],
    "test": [{ "eval": "!!document.cookie.match('MSCC')"}]
  }
